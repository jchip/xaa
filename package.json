{
  "name": "xaa",
  "version": "1.4.0",
  "description": "async/await/Promise helpers - delay, defer, timeout, each, map, filter",
  "main": "dist/xaa.js",
  "types": "dist/xaa.d.ts",
  "scripts": {
    "test": "clap -n -x compile test-only",
    "coverage": "clap -n -x compile check",
    "compile": "tsc",
    "prepublishOnly": "clap -n build docs",
    "docs": "typedoc --excludeNotExported --out docs src",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/xaa.git"
  },
  "keywords": [
    "async",
    "await",
    "Promise",
    "helpers",
    "async/await",
    "delay",
    "defer",
    "timeout",
    "each",
    "map",
    "filter"
  ],
  "files": [
    "dist"
  ],
  "author": "Joel Chen",
  "license": "Apache-2.0",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@xarc/module-dev": "^2.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "prettier": "^2.0.5",
    "run-verify": "^1.2.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "extends": [
      "@istanbuljs/nyc-config-typescript"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "*clap.js",
      "*clap.ts",
      "coverage",
      "dist",
      "docs",
      "gulpfile.js",
      "test"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": false
  },
  "prettier": {
    "printWidth": 100
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  },
  "dependencies": {}
}
